{
  "name": "ihs-riley/sage-lib",
  "license": "MIT",
  "description": "Forked version of Roots Sage-Lib",
  "type": "library",
  "authors": [
    {
      "name": "QWp6t",
      "email": "hi@qwp6t.me",
      "homepage": "https://github.com/qwp6t"
    },
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "homepage": "https://github.com/retlehs"
    }
  ],
  "keywords": [
    "wordpress"
  ],
  "autoload": {
    "psr-4": {
      "Roots\\Sage\\": ""
    }
  },
  "require": {
    "php": "*",
    "composer/installers": "~1.0",
    "illuminate/view": "~11",
    "illuminate/config": "~11"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "*"
  },
  "scripts": {
    "test": [
      "phpcs --ignore=vendor --extensions=php --standard=PSR2 ."
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
